install:
    npm install

serve:
    npm run serve

build:
    npm run build

test-unit:
    npm run test:unit

test-e2e:
    npm run test:e2e

lint:
    npm run lint

# Run on nginx
docker-build:
    docker build -t it-vibe-static-site:1.0 .

docker-run:
    docker run -d -zp 8080:80 it-vibe-static-site:1.0

terraform-apply-one-module:
    terraform plan -target=module.<module_name>